{
  "name": "packaging-options-pick-first-expo-config-plugin",
  "description": "Experimental Expo config plugin to add \"pick first\" config lines.",
  "version": "0.0.0",
  "engines": {
    "node": ">=16.11.1"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/packaging-options-pick-first-expo-config-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "del-cli": "4.0.1",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "scripts": {
    "buildClean": "del-cli \"**/*.js\" \"**/*.d.ts\" \"!**/node_modules/**/*\"",
    "buildLint": "eslint \"**/*.ts\"",
    "buildLintFix": "eslint --fix \"**/*.ts\"",
    "buildTypeScript": "tsc",
    "test": "npm-run-all buildClean buildLintFix buildTypeScript"
  },
  "files": [
    "app.plugin.js",
    "app.plugin.d.ts"
  ],
  "types": "app.plugin.d.ts",
  "sideEffects": false,
  "dependencies": {
    "@expo/config-plugins": "5.0.1"
  },
  "main": "app.plugin.js"
}
